<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yongchaobei.myapplication">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CustomViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.TESTACTION" />

                <category android:name="android.intent.category.TSET1" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CustonLayoutActivity">
            <intent-filter>
                <action android:name="android.intent.action.TESTACTION" />

                <category android:name="android.intent.category.TSET2" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BezirViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.TESTACTION" />

                <category android:name="android.intent.category.TSET3" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.JniTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.TESTACTION" />

                <category android:name="android.intent.category.TSET4" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>


</manifest>